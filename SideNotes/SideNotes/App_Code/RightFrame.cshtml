
@helper Render(string Header, string Id, string contentId)
{
<div class="rightFrameOuter" id="@Id">
    <div class="rightFrameHeader">
        <h2>@Header</h2>
        <a href="javascript:void(0)" class="rightFrameClose">x</a>
    </div>
    <div class="rightFrameContent" id="@contentId">
    </div>
</div>
}