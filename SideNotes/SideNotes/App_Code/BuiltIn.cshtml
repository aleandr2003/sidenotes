@using SideNotes.Models;
@helper Render(BuiltInCommentEnum value)
{
    switch(value)
    {
        case BuiltInCommentEnum.Plus1:
@:+1
            break;
        case BuiltInCommentEnum.Plus100500:
@:+100500
            break;
        case BuiltInCommentEnum.RjuNimagu:
@:Ржунимагу!
            break;
        case BuiltInCommentEnum.AfftarZjot:
@:Афтар ЖЖот!
            break;
        case BuiltInCommentEnum.VipeyYadu:
@:Выпей Йаду!
            break;
        case BuiltInCommentEnum.Tru:
@:Труъ!
            break;           
    }
}